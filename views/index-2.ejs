<!-- Home Section-->
<section id="home">
  <!-- Home Slider-->
  <div id="home-slider" class="flexslider">
    <ul class="slides">
      <% slide.forEach(element => { %>

      <li>
        <img src="<%= element.photo  %> " alt="" />
        <div class="slide-wrap">
          <div class="slide-content">
            <div class="container">
              <h1><%= element.title %> <span class="red-dot"></span></h1>
              <h6><%= element.subtitle %></h6>
              <p>
                <a href="#" class="btn btn-light-out"><%= element.left %> </a
                ><a href="#" class="btn btn-color btn-full"><%= element.right %> </a>
              </p>
            </div>
          </div>
        </div>
      </li>
      <% }) %>
    </ul>
  </div>
  <!-- End Home Slider-->
</section>
<!-- End Home Section-->
