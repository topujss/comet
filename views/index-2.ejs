<section id="portfolio" class="pb-0">
  <div class="container">
    <div class="row">
      <div class="col-md-6">
        <div class="title m-0 txt-xs-center txt-sm-center">
          <h2 class="upper">Selected Works<span class="red-dot"></span></h2>
          <hr />
        </div>
      </div>
      <div class="col-md-6">
        <ul id="filters" class="no-fix mt-25">
          <li data-filter="*" class="active">All</li>
          <li data-filter=".branding">Branding</li>
          <li data-filter=".graphic">Graphic</li>
          <li data-filter=".printing">Printing</li>
          <li data-filter=".video">Video</li>
        </ul>
        <!-- end of portfolio filters-->
      </div>
    </div>
    <!-- end of row-->
  </div>
  <div class="section-content pb-0">
    <div id="works" class="four-col wide mt-50">
      <% work.forEach(ele => { %> <% const tag = ele.tag %>

      <div class="work-item <%= tag %>">
        <div class="work-detail">
          <a href="<%= ele.link  %> ">
            <img src="<%= ele.photo  %> " alt="" />
            <div class="work-info">
              <div class="centrize">
                <div class="v-center">
                  <h3><%= ele.name %></h3>
                  <p><%= ele.displayTag %></p>
                </div>
              </div>
            </div>
          </a>
        </div>
      </div>
      <% }) %>
    </div>
    <!-- end of portfolio grid-->
  </div>
</section>
<section>
  <div class="container">
    <div class="title center">
      <h4 class="upper">Some of the best.</h4>
      <h3>Our Clients<span class="red-dot"></span></h3>
      <hr />
    </div>
    <div class="section-content">
      <div class="boxes clients">
        <div class="row">
          <% client.forEach(ele => { %> <% const bor = ele.border %>
          <div class="col-sm-4 col-xs-6 <%= bor %>">
            <img src="<%= ele.link  %>" alt="" data-animated="true" class="client-image" />
          </div>
          <% }) %>
        </div>
        <!-- end of row-->
      </div>
    </div>
    <!-- end of section content-->
  </div>
</section>
<section class="parallax">
  <div data-parallax="scroll" data-image-src="images/bg/7.jpg" class="parallax-bg"></div>
  <div class="parallax-overlay pb-50 pt-50">
    <div class="container">
      <div class="title center">
        <h3>What They Say<span class="red-dot"></span></h3>
        <hr />
      </div>
      <div class="section-content">
        <div
          id="testimonials-slider"
          data-options='{"animation": "slide", "controlNav": true}'
          class="flexslider nav-outside"
        >
          <ul class="slides">
            <% quote.forEach(ele => { %>
            <li>
              <blockquote>
                <p>"<%= ele.desc %>"</p>
                <footer><%= ele.person %></footer>
              </blockquote>
            </li>
            <% }) %>
          </ul>
        </div>
      </div>
    </div>
    <!-- end of container-->
  </div>
</section>
<section>
  <div class="container">
    <div class="title center">
      <h4 class="upper">We have a few tips for you.</h4>
      <h2>The Blog<span class="red-dot"></span></h2>
      <hr />
    </div>
    <div class="section-content">
      <div class="row">
        <div class="col-md-8 col-md-offset-2">
          <% blog.forEach(ele => { %>

          <div class="blog-post">
            <div class="post-body">
              <h3 class="serif"><a href="#"><%= ele.title %> </a></h3>
              <hr />
              <p class="serif"><%= ele.desc %></p>
              <div class="post-info upper">
                <a href="#">Read More</a><span class="pull-right black-text"><%= ele.time %> </span>
              </div>
            </div>
            <!-- end of blog post-->
          </div>
          <% }) %>
        </div>
      </div>
      <!-- end of row-->
      <div class="clearfix"></div>
      <div class="mt-25">
        <p class="text-center"><a href="/blog" class="btn btn-color-out">View Full Blog </a></p>
      </div>
    </div>
    <!-- end of container-->
  </div>
</section>
